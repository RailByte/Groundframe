  <data name="app_Usp_GET_USER_SETTINGSTest_TestAction.SqlScript" xml:space="preserve">
    <value>-- database unit test for app.Usp_GET_USER_SETTINGS
DECLARE @debug AS BIT = 0, @debug_session_id AS UNIQUEIDENTIFIER;

--Other Variables
DECLARE @error_message NVARCHAR(248) = N' No Error Raised';

--Check 1L Check Not Logged In Raises Error
BEGIN TRY
	EXEC [app].[Usp_GET_USER_SETTINGS] @debug, @debug_session_id OUTPUT;
END TRY
BEGIN CATCH
	SELECT @error_message = ERROR_MESSAGE();
END CATCH

--Result Set 1
SELECT
	[error_message] = @error_message;

--Log Standard User In

EXEC [common].[Usp_SET_SESSIONCONTEXT] @app_api_key = N'testappAPIKEY', @app_user_api_key = N'teststandarduserAPIKEY';
SET @error_message = N'No Error Raised';
DECLARE @app_user_id INT = ISNULL(CONVERT(INT,SESSION_CONTEXT(N'app_user')),0); 

DELETE FROM [app].[TUSERSETTING]
WHERE
	[user_id] =@app_user_id;

--Check 2: Check records are returned
--Result Set 2
EXEC [app].[Usp_GET_USER_SETTINGS] @debug, @debug_session_id OUTPUT;

--Check 3: Check user setting works

INSERT INTO [app].[TUSERSETTING]
(
	[user_id],
	[setting_id],
	[value]
)
VALUES
(
	@app_user_id,
	2,
	'fr-FR'
)
--Result Set 3
EXEC [app].[Usp_GET_USER_SETTINGS] @debug, @debug_session_id OUTPUT;

--Check 4: Check debug

SET @debug = 1
EXEC [app].[Usp_GET_USER_SETTINGS] @debug, @debug_session_id OUTPUT;

--Result Set 4
SELECT * FROM [audit].[TEVENT]
WHERE
	[debug_session_id] = @debug_session_id;</value>
